{
  "name": "aptivator-example",
  "version": "0.0.1",
  "dependencies": {
    "backbone": "~1.3.3",
    "backbone.marionette": "~2.4.0",
    "bootstrap": "~3.3.6",
    "aptivator": "file:../"
  },
  "devDependencies": {
    "browserify": "~13.1.0",
    "node-underscorify": "~0.0.14",
    "uglify-js": "~2.7.0",
    "lessify": "~1.0.1"
  },
  "scripts": {
    "build": "rm -rf node_modules/aptivator && npm install && browserify ./js/app.js > ./js/_app.js",
    "build-full": "browserify ./js/app.js > ./js/_app.js && uglifyjs ./js/_app.js --compress --mangled -o ./js/_app.js"
  },
  "browserify": {
    "transform": [
      [
        "node-underscorify",
        {
          "extensions": ["tpl"],
          "requires": [{
            "variable": "_",
            "module": "underscore"
          }]
        }
      ],
      [
        "lessify"
      ]
    ]
  }
}
