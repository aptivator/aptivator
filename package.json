    "jquery": "^2.0.0",
    "lodash": "~4.16.4"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-async-functions": "^6.22.0",
